                    function gdhs_search(bb) {
                        var s1 = document.getElementById(bb).value;
                        var s = escape(s1);
                        if (s1 == "输代码、名称或拼音" || s1 == "" || isNaN(parseInt(s1)) || s1.length != 6) {
                            alert("请输入所有查询股票的代码！");
                            return false;
                        } else {
                            var url = '/gdhs/detail/' + s1 + '.html';
                            window.open(url);
                        }
                    }
